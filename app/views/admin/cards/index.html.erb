<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header"> Admin - All Cards
      </h1>
    </div>
  </div>
  <% @cards.each do |card| %>
    <div class="row">
      <div class="col-md-7">
       <%= card.name %> | <%= link_to 'Edit', edit_admin_card_path(card) %>
      </div>
    </div>
    <hr>
  <% end %>
<li><%= link_to "Log Out", admin_logout_path, method: :delete %></li>
